<tr id="<%= dom_id search_engine_result %>" class="hover:bg-gray-50 dark:hover:bg-gray-800">
  <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 dark:text-white sm:pl-6">
    <%= search_engine_result.site_name %>
  </td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 dark:text-gray-400">
    <a href="<%= search_engine_result.url %>" target="_blank" class="text-indigo-600 hover:text-indigo-900 dark:text-indigo-400 dark:hover:text-indigo-300">
      <%= truncate(search_engine_result.url, length: 50) %>
    </a>
  </td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 dark:text-gray-400">
    <%= truncate(search_engine_result.title, length: 50) %>
  </td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 dark:text-gray-400">
    <span class="inline-flex items-center rounded-full bg-blue-100 dark:bg-blue-900 px-2.5 py-0.5 text-xs font-medium text-blue-800 dark:text-blue-200">
      #<%= search_engine_result.ranking %>
    </span>
  </td>
  <td class="relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-6">
    <div class="flex justify-end gap-2">
      <%= link_to [:app, search_engine_result], class: "text-indigo-600 hover:text-indigo-900 dark:text-indigo-400 dark:hover:text-indigo-300" do %>
        <svg class="h-5 w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
          <path d="M10 12a2 2 0 100-4 2 2 0 000 4z" />
          <path fill-rule="evenodd" d="M.458 10C1.732 5.943 5.522 3 10 3s8.268 2.943 9.542 7c-1.274 4.057-5.064 7-9.542 7S1.732 14.057.458 10zM14 10a4 4 0 11-8 0 4 4 0 018 0z" clip-rule="evenodd" />
        </svg>
      <% end %>
      <%= link_to [:edit, :app, search_engine_result], class: "text-indigo-600 hover:text-indigo-900 dark:text-indigo-400 dark:hover:text-indigo-300" do %>
        <svg class="h-5 w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
          <path d="M13.586 3.586a2 2 0 112.828 2.828l-.793.793-2.828-2.828.793-.793zM11.379 5.793L3 14.172V17h2.828l8.38-8.379-2.83-2.828z" />
        </svg>
      <% end %>
      <%= button_to [:app, search_engine_result], method: :delete, class: "text-red-600 hover:text-red-900 dark:text-red-400 dark:hover:text-red-300", form: { data: { turbo_confirm: "Are you sure?" } } do %>
        <svg class="h-5 w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
          <path fill-rule="evenodd" d="M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z" clip-rule="evenodd" />
        </svg>
      <% end %>
    </div>
  </td>
</tr>
